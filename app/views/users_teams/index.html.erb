<p id="notice"><%= notice %></p>

<h1>Listing Users Teams</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users_teams.each do |users_team| %>
      <tr>
        <td><%= users_team.user_id %></td>
        <td><%= users_team.team_id %></td>
        <td><%= link_to 'Show', users_team %></td>
        <td><%= link_to 'Edit', edit_users_team_path(users_team) %></td>
        <td><%= link_to 'Destroy', users_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Users team', new_users_team_path %>
